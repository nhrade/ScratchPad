{
    "manifest_version": 2,
    "name": "Scratchpad",
    "description": "Javascript scratchpad similar to firefox's scratchpad",
    "version": "0.0.1",
    "devtools_page": "devtools.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
  ]
}